{
  "name": "with-styletron",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ./**/*",
    "format": "prettier --write . --config ./.prettierrc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx}": [
      "yarn run lint:fix"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.5",
    "classnames": "^2.2.6",
    "graphql": "^15.4.0",
    "next": "latest",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-icons": "^3.11.0",
    "react-spring": "^8.0.27",
    "react-use-gesture": "^8.0.0",
    "styletron-engine-atomic": "^1.1.2",
    "styletron-react": "^5.0.1",
    "zustand": "^3.2.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@testing-library/dom": "^7.26.6",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.2",
    "@testing-library/user-event": "^12.2.2",
    "babel-jest": "^26.6.3",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.1.0",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1"
  }
}
